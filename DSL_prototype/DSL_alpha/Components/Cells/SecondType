define immovable cell SecondTypeCell from StemCell
    molecules:
        none

    constants:
        none

    compartments:
        none

    conditions:
        SecondTypeFactorSpreadCondition
            molecule SecondTypeFactor is in [0, 0.6]


    mechanisms:
        declaration:
            cyclic SpreadASymmetricFactor(duration: 250) from SpreadSignal
                parameters: (SecondTypeFactor)
                start on:
                    SecondTypeFactorSpreadCondition


        initial:
            SpreadASymmetricFactor

        mutually exclusive:
            #none
