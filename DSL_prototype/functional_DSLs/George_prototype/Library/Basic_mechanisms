# (деление) - применяется к объекту типа клетка
Division:
    availability: cell
    restrictions: not space, not connection, not compartment
    core_calls: cell_division

# (дифференциация) - применяется к объекту типа клетка
Differentiation:
    availability: cell
    restrictions: not space, not connection, not compartment
    core_calls: cell_differentiation

# (апоптоз) - применяется к объекту типа клетка
Apoptosis:
    availability: cell
    restrictions: not space, not connection, not compartment
    core_calls: cell_apoptosis

# (некроз) - применяется к объекту типа клетка
Necrosis:
    availability: cell
    restrictions: not space, not connection, not compartment
    core_calls: cell_nectosis

# (создание компартмента) - применяется к объектам типа клетка/компартмент
Compartment_creation:
    availability: cell, compartment
    restrictions: not space, not connection
    core_calls: compartment_formation

# (удаление компартмента) - применяется к объектам типа клетка/компартмент
Compartment_deletion:
    availability: cell, compartment
    restrictions: not space, not connection
    core_calls: compartment_deletion

Compartment_growth:
    availability: compartment
    restrictions: not space, not connection, not cell
    core_calls: compartment_growth

# (движение) - применяется к любому объекту, расположенному в пространстве
Moving:
    availability: object
    restrictions: not space, not connection
    core_calls: relocation

# (движение с "протискиванием"/обменом позиций) - применяется к любому объекту, расположенному в пространстве
Switch_moving:
    availability: object
    restrictions: not space, not connection
    core_calls: mutual_relocation

# (передача сигнала) - применяется к любому объекту. Бывает активный и пассивный транспорт сигнала.
Transfer_signal:
    availability: object
    restrictions:
    core_calls: signal_transfer

# (рост) - применяется к протяженным объектам типа аксон/дендрит
Growth:
    availability: axon, dendrite
    restrictions: not space, not connection, not cell, not compartment
    core_calls: compartment_growth

# (диффузия) - применяется к пространству (перетекание сигнала между пространством и безобъемным пространством осуществляется механизмом transfer signal)
Space_diffusion:
    availability: space
    restrictions: not cell, not compartment, not connection
    core_calls: signal_diffusion

# (заякоривание/разъякоривание) - применяется к клетке/компартменту
Anchoring:
    availability: cell, compartment
    restrictions: not space, not connection
    core_calls: anchor_creation, anchor_deletion

# (расчет динамической переменной) - применяется к любому объекту
Dynamic:
    availability: object
    restrictions:
    core_calls: dynamic