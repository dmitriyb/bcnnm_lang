define cell BasalProgenitorCell from StemCell
  movable: true
  signalingPathways:
    BPCToNRPDifferentiate(duration: 100) from Differentiate
      parameters: (NeuronalRestrictedProgenitorCell)

  conditions:
    BPCToNRPDifferentiateCondition
      condition: [NSCF > 0.0]
        
  cascades:
    on extracellular condition BPCToNRPDifferentiateCondition
      -> BPCToNRPDifferentiate