define cell RadialGliaCell from StemCell
  movable: true
  signalingPathways:
    RGCToGRPDifferentiate(duration: 100) from Differentiate
      parameters: (GlialRestrictedProgenitorCell)

  conditions:
    RGCToGRPDifferentiateCondition
      condition: [NSCF > 0.0]
        
  cascades:
    on extracellular condition RGCToGRPDifferentiateCondition
      -> RGCToGRPDifferentiate