#Define molecules

molecule C:                             #promotes the activation of cdc2 kinase through reversible dephosphorylation by E1
    type: cyclin
    radius: 0

molecule E1:                            #phosphatase E1, activated by cyclin, activates cdk2 through reversible dephosphorylation
    type: phosphatase
    radius: 0

molecule E2:                            #kinase E2 inactivates cdk2 (M) by reversible phosphorylation
    type: kinase
    radius: 0

molecule M_in:                          #M=cdk2=E3, in M_a (active) state activates cyclin protease (X) by reversible phosphorylation
    type: kinase
    radius: 0

molecule M_a:                           #M=cdk2=E3, in M_a (active) state activates cyclin protease (X) by reversible phosphorylation
    type: kinase
    radius: 0

molecule E4:                            #inactivates cyclin protease (X) through reversible dephosphorylation
    type: phosphatase
    radius: 0

molecule X_in:                          #in X_a (active) state degrades cyclin
    type: protease
    radius: 0

molecule X_a:                           #in X_a (active) state degrades cyclin
    type: protease
    radius: 0

