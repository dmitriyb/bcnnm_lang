define movable cell RadialGliaCell from StemCell
    molecules:
        none

    compartments:
        none

    conditions:
        RGCToGRPDifferentiateCondition
            molecule NSCF is in [0, infinity)

    mechanisms:
        declaration:
            single RGCToGRPDifferentiate(duration: 100) from Differentiate
                parameters: (GlialRestrictedProgenitorCell)
                start on:
                    RGCToGRPDifferentiateCondition

        initial:
            none

        mutually exclusive:
            none